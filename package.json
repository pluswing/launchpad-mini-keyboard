{
  "name": "launch_deck",
  "productName": "LanchDeck",
  "description": "",
  "version": "0.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluswing/launchpad-mini-keyboard"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./dist/index.html && npm run copy:assets && cross-env NODE_ENV=\"development\" electron .",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "webpack --watch --progress",
    "predev": "rimraf dist",
    "clean": "rimraf dist release",
    "serve": "electron .",
    "build": "cross-env NODE_ENV=\"development\" webpack --config webpack.config.prod.ts --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.prod.ts --progress",
    "build:pack": "ts-node ./scripts/build.ts",
    "package": "run-s clean build:prod copy:assets build:pack",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --write .",
    "prepare": "git config --local core.hooksPath hooks",
    "copy:assets": "node scripts/copy_assets.js"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^2.0.1",
    "electron-store": "^8.0.1",
    "keycode": "^2.2.1",
    "midi": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^2.2.4",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "ifdef-loader": "^2.3.2",
    "mini-css-extract-plugin": "^2.4.5",
    "node-loader": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "electron-notarize": "^1.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-search-devtools": "^1.5.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.0",
    "wait-on": "^6.0.0"
  },
  "volta": {
    "node": "16.13.0"
  }
}
